@model IEnumerable<GongChaWebApplication.Models.MemberUnavailability>


@if (Model.Count() > 0)
{
    foreach (var item in Model)
    {
    <div class="display-field">
        @if(item.IsWholeDay){
            @Html.Raw(item.StartHour.Value.Date.ToShortDateString()) @Html.Raw(" - ") @Html.Raw(item.EndHour.Value.Date.ToShortDateString())
        }else{
            @Html.DisplayFor(modelItem => item.StartHour) @Html.Raw(" - ") @Html.DisplayFor(modelItem => item.EndHour)
        }
    </div>
    }
}
else
{
    @Html.Raw("No data")
}